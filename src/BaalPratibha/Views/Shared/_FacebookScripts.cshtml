@model BaalPratibha.Models.ViewModels.ContestantView


<script type="text/javascript">
   
    function share() {
        FB.ui({
            method: 'share',
            href: '@Url.Action("Index", "Home",null,"http")@Model.UserName',
            hashtag: '#BaalPratibha'
        },
            function (response) {
                Window.refresh();
            });
    }


</script>